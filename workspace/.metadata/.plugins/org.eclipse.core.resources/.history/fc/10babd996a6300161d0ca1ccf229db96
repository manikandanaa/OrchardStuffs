import java.util.Scanner;


public class dsds
{
public static void main(String arg[])
{
	Scanner s=new Scanner(System.in);
	int a[]={3,1,2,5},temp;
	int i,j;
	int n=s.nextInt();
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			if(a[j]<a[j+1])
			{
				temp=a[j];
				a[j]=a[j+1];
				a[j+1]=temp;
			}
		}
	}
}
}
